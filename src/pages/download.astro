      <link rel="icon" type="image/svg+xml" href="../../public/favicon.svg">
      <style>
        body {
  margin: 0;
  font-family: Arial, sans-serif;
}
        .download-manager {
  width: 300px;
  margin: 20px auto;
}

.download-item {
  display: flex;
  align-items: center;
  padding: 10px;
  background-color: #f5f5f5;
  border-radius: 5px;
  margin-bottom: 10px;
}

.download-icon {
  font-size: 24px;
  margin-right: 10px;
}

.download-title {
  margin: 0;
}

.download-size {
  margin: 0;
  font-size: 14px;
  color: #666;
}

.download-button {
  background-color: #4caf50;
  color: white;
  border: none;
  padding: 8px 16px;
  border-radius: 4px;
  cursor: pointer;
}
nav {
        background-color: #3c8ce1;
        color: #fff;
      }

      nav ul {
        list-style-type: none;
        margin: 0;
        padding: 0;
        overflow: hidden;
      }

      nav li {
        float: left;
      }

      nav a {
        display: block;
        color: #fff;
        text-align: center;
        padding: 14px 16px;
        text-decoration: none;
      }

      nav a:hover {
        background-color: #555;
      }
      .marquee {
        background-color: yellow;
        color: black;
      }
      /* Style the details container */
    details {
      cursor: pointer;
    }

    /* Hide the default arrow */
    details summary::-webkit-details-marker {
      display: none;
    }

    /* Style the summary text */
    details summary {
      font-weight: bold;
      padding: 0.5em;
      border: 1px solid #ccc;
      background-color: #f0f0f0;
      border-radius: 5px;
    }

    /* Style the content inside the details */
    details > div {
      padding: 0.5em;
      border: 1px solid #ccc;
      border-top: none;
      border-radius: 0 0 5px 5px;
    }
    #border {
    border-style: double solid;
    }
      </style>
  </head>
